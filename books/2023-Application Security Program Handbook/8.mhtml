      <col class=3D"calibre18" span=3D"1" width=3D"16.66%">
    </colgroup>

    <tbody class=3D"calibre19">
      <tr class=3D"calibre20">
        <th class=3D"fm-contenttable2" colspan=3D"1" rowspan=3D"1">
          <p class=3D"fm-table-head"><a id=3D"pgfId-1009109"></a>Vulnerabil=
ity</p>
        </th>

        <th class=3D"fm-contenttable2" colspan=3D"1" rowspan=3D"1">
          <p class=3D"fm-table-head"><a id=3D"pgfId-1009111"></a>Date opene=
d</p>
        </th>

        <th class=3D"fm-contenttable2" colspan=3D"1" rowspan=3D"1">
          <p class=3D"fm-table-head"><a id=3D"pgfId-1009113"></a>Version de=
ployed</p>
        </th>

        <th class=3D"fm-contenttable2" colspan=3D"1" rowspan=3D"1">
          <p class=3D"fm-table-head"><a id=3D"pgfId-1009115"></a>Date deplo=
yed</p>
        </th>

        <th class=3D"fm-contenttable2" colspan=3D"1" rowspan=3D"1">
          <p class=3D"fm-table-head"><a id=3D"pgfId-1009117"></a>Number of =
days to remediate</p>
